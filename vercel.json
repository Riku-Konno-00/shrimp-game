{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/styles.css",
      "headers": [{ "key": "Content-Type", "value": "text/css" }]
    },
    {
      "source": "/src/game.js",
      "headers": [{ "key": "Content-Type", "value": "application/javascript" }]
    }
  ]
}